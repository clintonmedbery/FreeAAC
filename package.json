      "prettier --write",
      "./node_modules/.bin/eslint --fix",
      "git add"
    ],
    "utilities/**/*.{js,jsx}": [
      "prettier --write",
      "./node_modules/.bin/eslint --fix",
      "git add"
    ],
    "constants/**/*.{js,jsx}": [
      "prettier --write",
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "private": true
}
